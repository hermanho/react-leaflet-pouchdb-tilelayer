    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "build": "npm run lint && npm run build:types && npm run build:esm && npm run build:cjs",
    "transpile": "babel src -d dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
